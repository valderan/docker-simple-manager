# Docker Simple Manager environment configuration
# DEBUG включает расширенное логирование и дополнительные проверки
DEBUG=false
# LOG_LEVEL задаёт уровень логирования по умолчанию (INFO, DEBUG и т.д.)
LOG_LEVEL=INFO
# DSM_CONFIG_DIR путь к рабочей директории приложения
DSM_CONFIG_DIR=~/.dsmanager
# DSM_DOCKER_SOCKET сокет Docker, используемый по умолчанию для локальных соединений
DSM_DOCKER_SOCKET=unix:///var/run/docker.sock
